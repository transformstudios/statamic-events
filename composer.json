{
    "name": "transformstudios/events",
    "type": "statamic-addon",
    "license": "proprietary",
    "description": "Statamic addon to create & manage events",
    "authors": [
        {
            "name": "Erin Dalzell",
            "email": "erin@transformstudios.com",
            "homepage": "https://transformstudios.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TransformStudios\\Events\\": "src"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "TransformStudios\\Events\\Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Events",
            "description": "Statamic addon to create & manage events"
        },
        "laravel": {
            "providers": [
                "TransformStudios\\Events\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.0",
        "rlanvin/php-rrule": "^2.3",
        "simshaun/recurr": "^5.0",
        "spatie/calendar-links": "1.*",
        "statamic/cms": "^3.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "spatie/test-time": "^1.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "repositories": []
}
